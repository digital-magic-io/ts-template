{
  "name": "@digital-magic/ts-common-utils",
  "version": "1.0.1",
  "description": "Common utilities for Type Script projects",
  "main": "index.js",
  "typings": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./build",
    "lint": "tslint -p .",
    "jest-clear-cache": "jest --clearCache",
    "prettier": "pretty-quick --check --pattern \"src/**/*.*(ts|tsx)\" --pattern \"test/**/*.*(ts|tsx)\"",
    "fix-prettier": "pretty-quick --pattern \"src/**/*.*(ts|tsx)\" --pattern \"test/**/*.*(ts|tsx)\"",
    "jest": "jest",
    "test": "npm run jest-clear-cache && npm run prettier && npm run jest",
    "prebuild": "npm run clean && npm run prettier",
    "postbuild": "pretty-quick --pattern \"build/**/*.*(js|ts|jsx|tsx)\"",
    "precommit": "pretty-quick --staged",
    "prepare": "tsc -p .",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-magic-io/ts-template.git"
  },
  "author": "Jevgeni Goloborodko <jevgeni@digital-magic.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digital-magic-io/ts-template/issues"
  },
  "homepage": "https://github.com/digital-magic-io/ts-template#readme",
  "keywords": [
    "typescript",
    "template"
  ],
  "tags": [
    "typescript",
    "template"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^11.15.20",
    "@types/prettier": "^2.0.2",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "prettier": "^2.1.0",
    "pretty-quick": "^3.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "tslint-immutable": "^6.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
